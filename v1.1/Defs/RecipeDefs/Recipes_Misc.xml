<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>HolyWashApparel</defName>
    <label>Wash clothes</label>
    <description>Untaints apparel that has been worn by a corpse.</description>
    <jobString>washing clothes.</jobString>
    <workAmount>500</workAmount>
    <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
    <workSkill>Crafting</workSkill>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_WashingMachine</soundWorking>
    <recipeUsers>
      <li>HolyBasin</li>
      <li>HolyWasher</li>
    </recipeUsers>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Apparel</li>
          </categories>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <products>
      <Steel>1</Steel>
    </products>
    <fixedIngredientFilter>
      <categories>
        <li>Apparel</li>
      </categories>
      <specialFiltersToDisallow>
        <li>AllowNonDeadmansApparel</li>
      </specialFiltersToDisallow>
    </fixedIngredientFilter>
    <skillRequirements>
      <Crafting>1</Crafting>
    </skillRequirements>
  </RecipeDef>

</Defs>
